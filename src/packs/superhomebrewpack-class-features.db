{"_id":"8NvHPs0oxmi9V7yz","name":"Majestic Strike","type":"feat","img":"systems/dnd5e/icons/spells/enchant-magenta-1.jpg","effects":[],"flags":{"betterRolls5e":{"critRange":{"type":"String","value":""},"critDamage":{"type":"String","value":""},"quickDesc":{"type":"Boolean","value":true,"altValue":true},"quickAttack":{"type":"Boolean","value":true,"altValue":true},"quickSave":{"type":"Boolean","value":true,"altValue":true},"quickDamage":{"type":"Array","value":[],"altValue":[],"context":[]},"quickProperties":{"type":"Boolean","value":true,"altValue":true},"quickCharges":{"type":"Boolean","value":true,"altValue":true},"quickTemplate":{"type":"Boolean","value":false,"altValue":false},"quickOther":{"type":"Boolean","value":true,"altValue":true,"context":""},"quickFlavor":{"type":"Boolean","value":true,"altValue":true}},"dynamiceffects":{"equipActive":false,"alwaysActive":false,"effects":[]},"core":{"sourceId":"Compendium.SupersHomebrewPack.class-features.8NvHPs0oxmi9V7yz"}},"system":{"description":{"value":"<p style=\"box-sizing:border-box;user-select:text;color:#191813;font-size:13px\"><span style=\"color:#000000;font-size:14px\">At 11th level, you may now inspire yourself with your own majestic strikes. Whenever you hit a target creature, you may reduce your majesty score by 2 to deal an additional [[/r 2d6]] damage to that target with the same damage type as the weapon you struck with. You may further reduce your majesty score by 2 to increase this damage by 1d6, to a maximum of 5d6.</span></p>","chat":"","unidentified":""},"source":"Super's Pack","activation":{"type":"special","cost":null,"condition":"When you hit a target creature."},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":null,"per":"","recovery":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"part