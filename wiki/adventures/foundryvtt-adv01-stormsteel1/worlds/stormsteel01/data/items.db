eight":4,"price":"5","weaponType":"simpleM","bonus":{"value":""},"damage":{"parts":[["1d6 + @mod","bludgeoning"]]},"damageType":{"value":"bludgeoning"},"damage2":{"value":""},"damage2Type":{"value":""},"range":{"value":5,"units":"ft"},"properties":{"value":""},"proficient":false,"attuned":false,"ability":"str","activation":{"type":"action","cost":1},"actionType":"mwak"},"flags":{},"img":"systems/dnd5e/icons/items/weapons/morningstar.jpg","id":10,"canEdit":true,"totalWeight":4,"_id":"2nga239vpk1hqbq9"}]},"core":{"sheetClass":"dnd5e.ItemSheet5eWithBags","sourceId":"Compendium.itemcollection.packs.YIZxFSOZATN1yOSd"},"dynamiceffects":{"equipActive":false,"alwaysActive":false}},"system":{"description":{"value":"<ul style=\"box-sizing:inherit;-webkit-tap-highlight-color:transparent;outline:0px;list-style:initial;margin-top:15px;margin-bottom:15px;padding-left:25px;line-height:30px;font-family:Roboto, Helvetica, sans-serif;font-size:16px;background-color:#f9f9f9\">\n<li style=\"box-sizing:inherit;-webkit-tap-highlight-color:transparent;outline:0px\">(a) chain mail or (b) leather armor, longbow, and 20 arrows</li>\n<li style=\"box-sizing:inherit;-webkit-tap-highlight-color:transparent;outline:0px\">(a) a martial weapon and a shield or (b) two martial weapons</li>\n<li style=\"box-sizing:inherit;-webkit-tap-highlight-color:transparent;outline:0px\">(a) a light crossbow and 20 bolts or (b) two handaxes</li>\n<li style=\"box-sizing:inherit;-webkit-tap-highlight-color:transparent;outline:0px\">(a) a dungeoneer’s pack or (b) an explorer’s pack</li>\n</ul>","chat":"","unidentified":"","type":"String","label":"Description"},"source":"","quantity":1,"weight":111,"price":null,"attunement":0,"equipped":false,"rarity":"common","identified":true,"capacity":{"type":"weight","value":0,"weightless":false},"currency":{"cp":0,"sp":0,"ep":0,"gp":0,"pp":0},"damage":{"parts":[]},"critical":{"threshold":null,"damage":null}},"ownership":{"default":0,"g4WGw0lAZ3nIhapn":3,"a2um1mRnicEx282I":3},"_s